{
  "version": "2.0.1-*",
  "name": "Tiger.Types.Json",
  "title": "Tiger JSON Types",
  "description": "JSON serialization capabilites for Tiger Types.",
  "authors": [ "cosborn@cimpress.com" ],
  "copyright": "Copyright Â©Cimpress 2016",

  "buildOptions": {
    "debugType": "portable",
    "preserveCompilationContext": true,
    "xmlDoc": true,
    "warningsAsErrors": true,
    "define": [ "TRACE", "CODE_ANALYSIS", "CONTRACTS_FULL" ],
    "additionalArguments": [
      "/ruleset:../../Tiger.ruleset",
      "/additionalfile:../../stylecop.json"
    ]
  },
  "configurations": {
    "Debug": {
      "buildOptions": {
        "define": [ "DEBUG" ]
      }
    },
    "Release": {
      "buildOptions": {
        "define": [ "RELEASE" ],
        "optimize": true
      }
    }
  },

  "dependencies": {
    "Newtonsoft.Json": "9.0.1",
    "StyleCop.Analyzers": {
      "type": "build",
      "version": "1.0.0"
    },
    "Tiger.Types": "3.1.0"
  },

  "frameworks": {
    "netstandard1.0": {
      "dependencies": {
        "NETStandard.Library": "1.6.0"
      }
    }
  },

  "packOptions": {
    "releaseNotes": "* Thanks to the magic of the .NET Standard, Tiger libraries are available to a wider set of platforms than ever before. The project now targets .NET Standard 1.0.\r\n* The library has been renamed to reflect the growing nature of Tiger.",
    "owners": [ "cosborn@cimpress.com" ],
    "repository": {
      "type": "git",
      "url": "https://mcpstash.cimpress.net/projects/TIGER/repos/types-json"
    },
    "requireLicenseAcceptance": false,
    "tags": [ "option", "maybe", "either", "json", "functional" ],
    "iconUrl": "https://mcpstash.cimpress.net/projects/TIGER/repos/types-json/browse/tiger_logo.png?raw"
  }
}
